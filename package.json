  "version": "1.0.0",
  "description": "Easy to use Student Experience for Cisco WebexÂ®, Now extended to Windows, macOS, Linux Platforms!",
  "main": "dist/index.js",
  "scripts": {
    "start": "tsc && electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [],
  "author": {
    "name": "Alex4386",
