    "@types/tough-cookie": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "electron": "10.1.0",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "^2.20.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "axios-cookiejar-support": "^1.0.0",
    "cors": "^2.8.5",
